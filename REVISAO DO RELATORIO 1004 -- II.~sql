--------------VERSÃO 2.0
SELECT EDILD.DB_EDILD_NOMEARQ   AS "ARQ IMPORTADO",
       EDIP.DB_EDIP_LOTE        AS "LOTE MERCANET",
       EDIP.DB_EDIP_PEDMERC     AS "PEDIDO MERCANET",
       PED.DB_PED_NRO_ORIG      AS "PEDIDO JDE",
       PED.DB_PED_CLIENTE       AS "COD CLIENTE",
       EDIP.DB_EDIP_COMPRADOR   AS "CNPJ CLIENTE",
       CLI.DB_CLI_NOME          AS "RAZAO SOCIAL",
       PEDI.DB_PEDI_PRODUTO     AS "COD PRODUTO",
       EDII.DB_EDII_PRODUTO     AS "EAN PRODUTO",
       PROD.DB_PROD_DESCRICAO   AS "DESCRICAO DO PRODUTO",
       PEDI.DB_PEDI_QTDE_SOLIC  AS "QTDE SOLICITADA",
       PEDI.DB_PEDI_QTDE_ATEND  AS "QTDE ATENDIDA",
       PEDI.DB_PEDI_QTDE_CANC   AS "QTDE NÃO ATENDIDA",
       EDIP.DB_EDIP_PROJETO     AS "CODIGO PROJETO",
       LAB.DB_LAB_DESCRICAO     AS "FORNECEDOR",
       EMP.DB_TBEMP_NOME        AS "FILIAL DE VENDA" 
  FROM DB_EDI_LOTE_DISTR EDILD,
       DB_EDI_PEDIDO     EDIP,
       DB_EDI_PEDPROD    EDII,
       DB_PEDIDO         PED,
       DB_PEDIDO_PROD    PEDI,
       DB_CLIENTE        CLI,
       DB_LABORATORIO    LAB,
       DB_TB_EMPRESA     EMP,
       DB_PRODUTO        PROD,
       DB_PRODUTO_EMBAL  PRDEMB
 WHERE EDILD.DB_EDILD_SEQ       = EDIP.DB_EDIP_LOTE
   AND EDIP.DB_EDIP_COMPRADOR  = EDII.DB_EDII_COMPRADOR
   AND EDIP.DB_EDIP_NRO        = EDII.DB_EDII_NRO
   AND EDIP.DB_EDIP_PEDMERC    = PED.DB_PED_NRO
   AND PED.DB_PED_NRO         = PEDI.DB_PEDI_PEDIDO
   AND PED.DB_PED_CLIENTE     = CLI.DB_CLI_CODIGO
   AND EDIP.DB_EDIP_LABCOD     = LAB.DB_LAB_CODIGO
   AND PED.DB_PED_EMPRESA     = EMP.DB_TBEMP_CODIGO
   AND PRDEMB.DB_PRDEMB_PRODUTO  = PEDI.DB_PEDI_PRODUTO
   AND PEDI.DB_PEDI_PRODUTO    = PROD.DB_PROD_CODIGO
   AND EDII.DB_EDII_PRODUTO    = PRDEMB.DB_PRDEMB_CODBARRA
   AND PEDI.DB_PEDI_QTDE_ATEND > 0 
   AND NOT EXISTS (SELECT *
                     FROM DB_NOTA_FISCAL NF
                     WHERE PED.DB_PED_NRO_ORIG = NF.DB_NOTA_PED_ORIG
                     AND PED.DB_PED_NRO      = NF.DB_NOTA_PED_MERC
                     AND PED.DB_PED_CLIENTE  = NF.DB_NOTA_CLIENTE
                     AND PED.DB_PED_EMPRESA  = NF.DB_NOTA_EMPRESA)
                     -- AND DB_EDIP_VAN = 501 -- CRIAR FILTRO DE VAN
                     -- AND EDIP.DB_EDIP_PEDMERC    =     -- CRIAR FILTRO DE PEDIDO
                     -- AND DB_EDIP_LOTE > 286360
                     -- AND EDIP.DB_EDIP_OBS1       =     -- CRIAR FILTRO DE LOTE SEVEN
                     -- AND EDIP.DB_EDIP_CLIENTE    =     -- CRIAR FILTRO DE CNPJ DE CLIENTE
                     -- AND EDIP.DB_PED_CLIENTE     =     -- CRIAR FILTRO DE CODIGO DO CLIENTE
                     -- AND EDII.DB_EDII_PRODUTO    =     -- CRIAR FILTRO DE CODIGO DE BARRAS DO PRODUTO
                     -- AND PEDI.DB_PEDI_PRODUTO    =     -- CRIAR FILTRO DE CODIGO DO PRODUTO
   AND DB_EDILD_DATA BETWEEN
                     TO_DATE('01/05/2012 00:00:00', 'DD/MM/YYYY HH24:MI:SS') AND
                     TO_DATE('06/06/2012 23:59:59', 'DD/MM/YYYY HH24:MI:SS')
  ORDER BY A.DB_EDIP_LOTE, A.DB_EDIP_PEDMERC


----------------------- VERSÃO 1.0
SELECT DB_EDILD_NOMEARQ   AS "ARQ IMPORTADO",
       DB_EDIP_LOTE       AS "LOTE MERCANET",
       DB_EDIP_PEDMERC    AS "PEDIDO MERCANET",
       DB_PED_NRO_ORIG    AS "PEDIDO JDE",
       DB_PED_CLIENTE     AS "COD CLIENTE",
       DB_EDIP_COMPRADOR  AS "CNPJ CLIENTE",
       DB_CLI_NOME        AS "RAZAO SOCIAL",
       DB_PEDI_PRODUTO    AS "COD PRODUTO",
       DB_EDII_PRODUTO    AS "EAN PRODUTO",
       DB_PROD_DESCRICAO  AS "DESCRICAO DO PRODUTO",
       DB_PEDI_QTDE_SOLIC AS "QTDE SOLICITADA",
       DB_PEDI_QTDE_ATEND AS "QTDE ATENDIDA",
       DB_PEDI_QTDE_CANC  AS "QTDE NÃO ATENDIDA",
       DB_EDIP_PROJETO    AS "CODIGO PROJETO",
       DB_LAB_DESCRICAO   AS "FORNECEDOR",
       DB_TBEMP_NOME      AS "FILIAL DE VENDA"
  FROM DB_EDI_LOTE_DISTR,
       DB_EDI_PEDIDO,
       DB_EDI_PEDPROD,
       DB_PEDIDO,
       DB_PEDIDO_PROD,
       DB_CLIENTE,
       DB_LABORATORIO,
       DB_TB_EMPRESA,
       DB_PRODUTO,
       DB_PRODUTO_EMBAL
 WHERE DB_EDILD_SEQ = DB_EDIP_LOTE
   AND DB_EDIP_COMPRADOR = DB_EDII_COMPRADOR
   AND DB_EDIP_NRO = DB_EDII_NRO
   AND DB_EDIP_PEDMERC = DB_PED_NRO
   AND DB_PED_NRO = DB_PEDI_PEDIDO
   AND DB_PED_CLIENTE = DB_CLI_CODIGO
   AND DB_EDIP_LABCOD = DB_LAB_CODIGO
   AND DB_PED_EMPRESA = DB_TBEMP_CODIGO
   AND DB_PRDEMB_PRODUTO = DB_PEDI_PRODUTO
   AND DB_PEDI_PRODUTO = DB_PROD_CODIGO
   AND DB_EDII_PRODUTO = DB_PRDEMB_CODBARRA
   AND NOT EXISTS (SELECT *
          FROM DB_NOTA_FISCAL
         WHERE DB_PED_NRO_ORIG = DB_NOTA_PED_ORIG
           AND DB_PED_NRO = DB_NOTA_PED_MERC
           AND DB_PED_CLIENTE = DB_NOTA_CLIENTE
           AND DB_PED_EMPRESA = DB_NOTA_EMPRESA)
-- AND DB_EDIP_VAN = 501 -- CRIAR FILTRO DE VAN
-- AND EDIP.DB_EDIP_PEDMERC    =     -- CRIAR FILTRO DE PEDIDO
-- AND DB_EDIP_LOTE > 286360
-- AND EDIP.DB_EDIP_OBS1       =     -- CRIAR FILTRO DE LOTE SEVEN
-- AND EDIP.DB_EDIP_CLIENTE    =     -- CRIAR FILTRO DE CNPJ DE CLIENTE
-- AND EDIP.DB_PED_CLIENTE     =     -- CRIAR FILTRO DE CODIGO DO CLIENTE
-- AND EDII.DB_EDII_PRODUTO    =     -- CRIAR FILTRO DE CODIGO DE BARRAS DO PRODUTO
-- AND PEDI.DB_PEDI_PRODUTO    =     -- CRIAR FILTRO DE CODIGO DO PRODUTO
--AND DB_EDILD_DATA BETWEEN
       TO_DATE('01/06/2012 00:00:00', 'DD/MM/YYYY HH24:MI:SS') AND
       TO_DATE('06/06/2012 23:59:59', 'DD/MM/YYYY HH24:MI:SS')
