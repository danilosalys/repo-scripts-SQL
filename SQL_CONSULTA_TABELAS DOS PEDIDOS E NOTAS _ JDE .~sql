SELECT SDDOCO , SDDOC , sdnxtr ,sdlttr, SDDCTO FROM PRODDTA.F42119
WHERE SDDOCO = 12942107;

SELECT * FROM PRODDTA.F7611B
WHERE FDDOCO = 12942107;

SELECT * FROM PRODDTA.F55NFE02
WHERE NCBNNF IN (76790,76759)
AND NCBSER IN(14)
AND NCN001 IN (14386705,14392136);

SELECT * FROM PRODDTA.F55NFE03
WHERE NEBNNF IN (76790,76759)
AND NEBSER IN(14)
AND NEN001 IN (14386705,14392136);

SELECT * FROM PRODDTA.F5555NET
WHERE NTBNNF IN (76790,76759)
AND NTBSER IN (14)
AND NTN001 IN (14386705,14392136);
 
--INSERIDO APOS O PROCESSO DA ROTINA 14
SELECT * FROM PRODDTA.F55MCN01
WHERE FHBNNF IN (76790,76759)
AND FHBSER IN(14)
AND FHN001 IN(14386705,14392136);

SELECT * FROM PRODDTA.F554201R
WHERE RTDOCO  = 11509430
ORDER BY rtdcto, RTLNID , RTNXTR;

