SELECT SDOORN,
       SDDOCO,
       SDDCTO,
       SDLITM,
       SDLOCN,
       SDLOTN,
       SDLNTY,
       SDAN8,
       SDZON,
       SDLTTR,
       SDNXTR,
       SDEMCU,
       sdmcu,
       SDDOC,
       SDURAT,
       SDURAB,
       SHHOLD
  FROM QADTA.F4211, QADTA.F4201
 WHERE SDDOCO = SHDOCO
   AND SDDCTO = SHDCTO
   AND SHHOLD = ' '
   AND SDLTTR NOT IN ('980', '982')
   AND SDLNTY = 'BS'
   AND SDDOCO IN (7278294, 7278295, 7278297, 7278303, 7278304, 7278313,
